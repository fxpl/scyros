override def compute(x: Double): Double = {
        x match {
            case Double.PositiveInfinity => 0.0
            case Double.NegativeInfinity => 0.0
            case d if d.isNaN            => -1.0
            case d if d > 0              => sqrt(d) + log(d)
            case d if d < 0              => abs(d) * sin(d)
            case _                       => 0.0
        }
    }